{
	"@module": "/script/game-rpg.js",
	"@class": "TitleScene",
	"id": "title",
	"objects": [
		{
			"@module": "/script/game-rpg.js",
			"@class": "Arrangement",
			"name": "arrangement",
			"layerType": "collision",
			"layerOrder": ["ground", "field", "sky"]
		},
		{
			"@module": "/script/game-rpg.js",
			"@class": "Map",
			"name": "map",
			"layers": {
				"collision": "ground"
			},
			"tags": ["ground"],
			"components": [],
			"transform": {
				"size": [2000, 2000]
			}
		},
		{
			"@module": "/script/uengine.js",
			"@class": "UGameObject",
			"name": "player",
			"layers": {
				"collision": "field"
			},
			"tags": ["player"],
			"components": [
				{
					"@module": "/script/game-rpg.js",
					"@class": "SpriteRenderer",
					"name": "spriteRenderer",
					"sprite": "@asset-example/sprites/sprite-logo"
				},
				{
					"@module": "/script/game-rpg.js",
					"@class": "Stats",
					"name": "stats",
					"speed": 500
				},
				{
					"@module": "/script/uengine.js",
					"@class": "UContext",
					"name": "state",
					"components": [
						{
							"@module": "/script/game-rpg.js",
							"@class": "PlayerStateIdle",
							"name": "idle"
						},
						{
							"@module": "/script/game-rpg.js",
							"@class": "PlayerStateMove",
							"name": "move"
						},
						{
							"@module": "/script/game-rpg.js",
							"@class": "PlayerStateProbe",
							"name": "probe"
						}
					]
				},
				{
					"@module": "/script/game-rpg.js",
					"@class": "PlayerController"
				},
				{
					"@module": "/script/game-rpg.js",
					"@class": "PlayerView"
				}
			],
			"transform": {
				"position": [100, 100],
				"size": [50, 35]
			},
			"anchor": [0.5, 0.5]
		},
		{
			"@module": "/script/game-rpg.js",
			"@class": "RandomCharacterGenerator"
		},
		{
			"@module": "/script/uengine.js",
			"@class": "UCamera",
			"name": "camera",
			"transform": {
				"position": [0, 0],
				"size": [500, 500]
			},
			"anchor": [0.5, 0.5]
		}
	]
}