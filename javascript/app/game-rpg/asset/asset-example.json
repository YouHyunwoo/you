{
    "values": {
        "name": "YouHyunwoo"
    },
    "images": {
        "@module": "/script/uengine.js",
        "@class": "UImage",
        "image-logo": { "file": "image/logo.png" },
        "image-stone": { "file": "image/stone.png" },
        "image-trees": { "file": "image/trees.png" },
        "image-squirrel": { "file": "image/squirrel.png" },
        "image-youtube": { "file": "image/youtube.png" }
    },
    "sprites": {
        "@module": "/script/uengine.js",
        "@class": "USprite",
        "sprite-logo": {
            "sheet": "@images/image-logo",
            "source": [0, 0, null, null],
            "anchor": [0.5, 1],
            "scale": [0.25, 0.5]
        },
        "sprite-stone": {
            "sheet": "@images/image-stone",
            "source": [0, 0, null, null],
            "anchor": [0.55, 1],
            "scale": [0.1, 0.1]
        },
        "sprite-tree-0": {
            "sheet": "@images/image-trees",
            "source": [0, 0, 64, 64],
            "anchor": [0.5, 0.95],
            "scale": [5, 5]
        },
        "sprite-tree-1": {
            "sheet": "@images/image-trees",
            "source": [64, 0, 64, 64],
            "anchor": [0.5, 0.95],
            "scale": [7, 7]
        },
        "sprite-squirrel-idle": {
            "sheet": "@images/image-squirrel",
            "source": [0, 0, 48, 32],
            "anchor": [0.25, 0.95],
            "scale": [1, 1]
        },
        "sprite-squirrel-move": {
            "sheet": "@images/image-squirrel",
            "source": [0, 32, 48, 32],
            "anchor": [0.25, 0.95],
            "scale": [1, 1]
        }
    },
    "objects": {
        "tree-0": {
            "@module": "/script/uengine.js",
            "@class": "UGameObject",
            "name": "tree",
            "layers": {
                "collision": "field"
            },
            "tags": ["block"],
            "transform": {
                "size": [50, 35]
            },
            "anchor": [0.5, 0.5],
            "components": [
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "SpriteRenderer",
                    "name": "sprite renderer",
                    "sprite": "@sprites/sprite-tree-0"
                },
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "Stats",
                    "name": "stats",
                    "hp": 10,
                    "maxhp": 10,
                    "dp": 1
                }
            ]
        },
        "tree-1": {
            "@module": "/script/uengine.js",
            "@class": "UGameObject",
            "name": "tree",
            "layers": {
                "collision": "field"
            },
            "tags": ["block"],
            "transform": {
                "size": [50, 35]
            },
            "anchor": [0.5, 0.5],
            "components": [
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "SpriteRenderer",
                    "name": "sprite renderer",
                    "sprite": "@sprites/sprite-tree-1"
                },
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "Stats",
                    "name": "stats",
                    "hp": 10,
                    "maxhp": 10,
                    "dp": 1
                }
            ]
        },
        "stone": {
			"@module": "/script/uengine.js",
			"@class": "UGameObject",
			"name": "stone",
			"layers": {
				"collision": "field"
			},
			"components": [
				{
					"@module": "/script/game-rpg.js",
					"@class": "SpriteRenderer",
					"name": "spriteRenderer",
					"sprite": "@sprites/sprite-stone"
				}
			],
			"transform": {
				"position": [30, 30],
				"size": [10, 7]
			},
			"anchor": [0.5, 0.5]
        },
        "squirrel": {
            "@module": "/script/game-rpg.js",
            "@class": "Character",
            "name": "squirrel",
            "layers": {
                "collision": "field"
            },
            "tags": ["block"],
            "transform": {
                "size": [50, 35]
            },
            "anchor": [0.5, 0.5],
            "components": [
                {
					"@module": "/script/game-rpg.js",
					"@class": "SpriteRenderer",
					"name": "spriteRenderer",
					"sprite": "@sprites/sprite-squirrel-idle"
                },
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "Moveable",
                    "name": "moveable"
                },
				{
					"@module": "/script/game-rpg.js",
					"@class": "Stats",
                    "name": "stats",
                    "hp": 3,
                    "maxhp": 3,
                    "ap": 1,
                    "sight": 200,
					"speed": 100
                },
                {
                    "@module": "/script/game-rpg.js",
                    "@class": "AI",
                    "name": "ai",
                    "aggressive": -0.3,
                    "aggressiveImage": "@images/image-youtube"
                }
            ]
        }
    }
}